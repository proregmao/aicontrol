# 开发环境配置
NODE_ENV=development

# 前端开发服务器端口
VITE_PORT=3005
VITE_HOST=localhost

# API基础URL (从根目录.env读取，如果没有则使用默认值)
VITE_API_BASE_URL=http://localhost:8080/api/v1

# WebSocket URL
VITE_WS_URL=ws://localhost:8080/ws

# 应用标题
VITE_APP_TITLE=智能设备管理系统

# 是否启用Mock数据
VITE_USE_MOCK=false

# 是否启用开发工具
VITE_DEV_TOOLS=true
